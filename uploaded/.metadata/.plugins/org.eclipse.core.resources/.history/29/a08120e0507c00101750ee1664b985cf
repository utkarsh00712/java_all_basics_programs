import java.util.Scanner;
public class TwoDimensionalStringArray {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter the name of class and Students");
	
	String arr[][] = new String[sc.nextInt()][sc.nextInt()];
	
	for(int i = 0; i <= arr.length-1; i++) {
		for(int j = 0; j<= arr[i].length-1; i++) {
			System.out.println("Enter the name of class " + i + " Student " + j);
			arr[i][j] = sc.nextLine();		
			}
	}
	
	for(int i = 0; i <= arr.length-1; i++) {
		System.out.println("[");
		for(int j = 0; j <= arr[i].length-1; j++) {
			System.out.print(arr[i][j]);
			if(j != arr[i].length-1) {
				System.out.print(",");
			}
		}
		System.out.println("]");
	}
}
}
